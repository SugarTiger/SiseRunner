body{
	font-family: "微软雅黑";
	min-width: 720px;
}
.mynav{
	text-align:center;
}
.mynav ul{
	display: inline-block;
}
.mynav li{
	width: 150px;
	height: 50px;
	line-height: 30px;
	text-align: center;
	font-size: 20px;
	font-family: "微软雅黑";
	background-color: #009944;
	margin-bottom: 50px;
	border-radius: 4px;
	font-weight: bolder;
}
.mynav li a{
	color: #FFFFFF;
}
.mynav li a:hover{
	/*font-weight: bolder;*/
	background-color: #009944;
	color: #FFFFFF;
	box-shadow:0px 0px 1px 1px #029758;
}
.mynav li a:visited{
	background-color: #009944;
	color: #FFFFFF;
}
.nav-pills>li+li {
	margin-left: 15px
}
.myimg{
	margin-bottom: 30px;
	margin-right: 15px;
	width:300px;
}
.content h2{
	font-family: "微软雅黑";
	color: #484848;
}
.content p{
	margin-top: 20px;
	font-family: "宋体";
	color: #868585;
	line-height: 25px;
}
a:link{
	text-decoration: none;
}
.btn-custom{
	color: #000000;
	background-color: #D9E6DF;
	border-color: #4cae4c;
	margin-bottom: 30px;
	height: 40px;
	font-size: 20px;
	cursor: default;
	
}
