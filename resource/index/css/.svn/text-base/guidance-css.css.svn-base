.title{
	width: 100%;
	height: 100px;
	margin-top: 15px;
	margin-bottom: 30px;
}
.title1{
	border: 3px solid #009944;
	width:40%;
	height: 100px;
	border-left: none;
	border-bottom: none;
	border-right: none;
	float: left;
}
.title2{
	border: 3px solid #009944;
	width:20%;
	height: 100px;
	border-top: none;
	font-family:"微软雅黑";
	text-align: center;
	padding-top: 30px;
	display: block;
	float: left;
}
.title3{
	border: 3px solid #009944;
	width:40%;
	height: 100px;
	border-left: none;
	border-right: none;
	border-bottom: none;
	float: right;
	font-family: "宋体";
	text-align: right;
	padding-top: 80px;
}
.title3 a{
	color: #000000;
}
.title3 img{
	width: 25px;
}
.caption a h4{
	font-weight: bold;
	color: #333;
}
.caption p,
p a{
	color: #4B4C4C;
	font-size: 16px;
	font-weight: bold;
}
.img-head{
	width: 70px;
	height: 70px;
	margin-right: 20px;
	border-radius: 50%;
	-moz-border-radius: 50%;
    -webkit-border-radius: 50%;
}
.img-like{
	float: right;
	margin-right: 25px;
	margin-top: 25px;
}
.thumbnail img{
	width: 100%
}
.pagination>.active>a,
.pagination>.active>a:hover,
.pagination>.active>a:focus {
	background-color: #01aa4c;
	border-color: #02c559;
}
.pagination>li>a{
	color: #4B4C4C;
	font-size: 20px;
}