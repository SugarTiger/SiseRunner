.check-div {
	height: 70px;
	line-height: 70px;
	padding-left: 30px;
	box-sizing: border-box;
	margin-top: -40px;
	
}
.check-div button {
	font-size: 12px;
	font-weight: bold;
	border: 1px solid #008800;
	color: #FFFFFF;
}
.check-div button:hover{
	color: #FFFFFF;
}
.check-div select {
	height: 26px;
	width: 120px!important;
	display: inline;
	color: #ccc;
}
.check-div input {
	width: 200px !important;
	display: inline;
}
.theme-popover {
	z-index:9999;
	position:fixed;
	top:50%;
	left:50%;
	width:300px;
	height:200px;
	margin:-150px 0 0 -110px;
	border-radius:5px;
	border:solid 2px #666;
	background-color:#fff;
	display:none;
	box-shadow: 0 0 10px #666;
}
.theme-poptit {
	border-bottom:1px solid #ddd;
	padding:12px;
	position: relative;
}
.theme-popbod {
	padding:30px;
	color:#444;
	height: 148px;
}
.theme-popbom {
	padding:15px;
	background-color:#f6f6f6;
	border-top:1px solid #ddd;
	border-radius:0 0 5px 5px;
	color:#666
}
.theme-signin {
	margin: 0px 20px 0px 70px;
	text-align:left;
	font-size: 14px;
}
.theme-signin h4 {
	color:#999;
	font-weight:100;
	margin-bottom: 20px;
	font-size: 12px;
}
.theme-signin li {
	padding-left: 10px;
}
.theme-signin li strong {
	float: left;
	margin-left: -80px;
	width: 80px;
	text-align: right;
	line-height: 32px;
}
.theme-signin .btn {
	margin-top: 20px;
	margin-left: 30px;
}

