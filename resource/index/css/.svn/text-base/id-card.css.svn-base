.postblog{background: #fff;border: 1px solid #e2e2e2}
.postblog #title{
	width: 382px;
    font-size: 30px;
    color: #8fabb8;
    height: auto;
    background: none;
    border: 0;
    box-shadow: none;
    line-height: 1;
    padding: 35px 0 22px 35px;
    width: 100%;}
.postblog #title::-webkit-input-placeholder { color: #8fabb8; } 
.postblog #title:-moz-placeholder {color: color: #8fabb8; } 
.postblog #title::-moz-placeholder {color: color: #8fabb8; } 
.postblog #title:-ms-input-placeholder { color: #8fabb8; } 
.postblog #hdtitle{width: 382px;font-size: 30px;color: #8fabb8;height: auto;background: none;border: 0;box-shadow: none;line-height: 1;padding: 35px 0 22px 35px;width: 100%;}
.postblog #hdtitle::-webkit-input-placeholder { color: #8fabb8; } 
.postblog #hdtitle:-moz-placeholder {color: color: #8fabb8; } 
.postblog #hdtitle::-moz-placeholder {color: color: #8fabb8; } 
.postblog #hdtitle:-ms-input-placeholder { color: color: #8fabb8; } 
.torunbox{border-top: 1px solid #e2e2e2;padding: 24px 0 20px 0;}
.torunbox .dhcell{text-align: left;}
.torunbox .dhrow:nth-child(1) .dhcell{text-align: left;}
.torunbox .dhrow:nth-child(1) .dhcell{background: #fff;color: #333}
.torunbox .dhrow .dhcell:nth-child(1){width: 16%;text-align: right;}
.torunbox .dhrow:nth-child(1) .dhcell:nth-child(1){padding: 8px 10px 8px 0px}
.torunbox .dhrow:not(:nth-child(1)) .dhcell:nth-child(1){padding: 8px 10px 8px 0px}
.torunbox .dhrow:not(:nth-child(1)) .dhcell{border: 0}
#hdtitle{width: 100%;}

label{display:inline-block;margin-bottom:5px;}
.v3dh{width: 100%;}
.dhlistbox{width: 100%;display: table}
.dhlistbox .dhrow{display: table-row}
.dhlistbox .dhcell{display: table-cell;text-align: center;}
.dhlistbox .dhrow .dhcell:nth-child(1){width: 50%;text-align: left;}
.dhlistbox .dhrow:nth-child(1) .dhcell{text-align: center;background: #f7f7f7;line-height: 28px;color: #959595}
.dhlistbox .dhrow .dhcell:nth-child(1) img{float: left;width: 74px;border: 1px solid #d4d2d3;margin-right: 10px;margin-top: -14px;}
.dhlistbox  a{color: #333}
.dhlistbox  a:hover{color: #0e90d2}
