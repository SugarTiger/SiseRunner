.details-section,.details-aside{
	background-color: #FFFFFF;
}

.single-top,.single-top-in{
	margin-top: 2em;
}

.display-img-bor1,.display-img-bor2{
	float: left;
	width: 315px;
	border: solid #339933 1px;
}
.display-img-bor2{
	border-top: none;
}
.display1-img{
	width:314px;
	height: 350px;
	padding: 0.1em;
}

.display2-img{
	width: 95px;
	height: 130px;
	margin-left: 40px;
	float: left;
	padding: 0.1em;
}
.hor-line{
	border-top:1px #339933 solid;
	float: left;
	width:100%; 
	margin-top: 20px;
	margin-bottom: 20px;
}

.tailstop,.tailsbot{
	float: left;
	font-family: arial;
	margin-left: 25px;
}
.gtitleh3{
	font-weight: bolder;
	font-size: 24px;
	color: #5E5E5E;
}

.gtitleh4{
	margin-top: 1.2em;
	font-size: 22px;
}

.tailstop-btn{
	float: right;
	margin-right: -4em;
	margin-top: -3.5em;
}

.tailstop-btn button{
	margin-top: 0.5em;
	background-color: #339933 ;
	font-size: 13px;
	color: #FFFFFF;
	width: 120px;
	height:2em;
	border: none;
	font-weight: 200;
	border-radius: 5px;
}

.tailsbot h5,.goodsintr{
	font-size: 16px;
	font-weight: 600;
}

select{
	font-size: 13px;
	width:110px;
}

.area{
	margin-left: 86px;
	}
	

.buynow{
	background-color: #339933 ;
	font-size: 20px;
	color: #FFFFFF;
	width: 140px;
	height:2.4em;
	border: none;
	font-weight: 200;
	border-radius: 5px;
	margin-top: 20px;
	margin-left: 100px;
}
.goodsintr{
	margin-right: 20px;
	margin-top: 30px;
}

.masbox{
	width: 100%;
	height: 60px;
	background-color: #F2F2F2;
	margin-top: 15px;
}

.masbox img{
	float: left;
	width: 45px;
	margin-top: 8px;
	margin-left: 12px;
	margin-right: 12px;
}

.message{
	float: left;
	width: 100%;
}
